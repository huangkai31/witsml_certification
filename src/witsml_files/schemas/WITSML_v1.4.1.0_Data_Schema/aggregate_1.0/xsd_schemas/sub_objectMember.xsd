<!-- Example import of all the WITSML object schemas:
	<xsd:import namespace="http://www.witsml.org/schemas/1series" 
		    schemaLocation="../../witsml_v1.4.1_data/xsd_schemas/sub_objectMemberWitsml.xsd"/>
